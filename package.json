{
  "name": "@olivierjm/react-mounter",
  "version": "1.2.1",
  "description": "React Mounter lets you mount React components to DOM easily.",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/olivierjm/react-mounter.git"
  },
  "license": "MIT",
  "options": {
    "mocha": "--require scripts/mocha_runner lib/**/__tests__/**/*.js"
  },
  "scripts": {
    "prepublish": ". ./scripts/prepublish.sh",
    "lint": "eslint ./lib",
    "lintfix": "eslint ./lib --fix",
    "testonly": "mocha $npm_package_options_mocha",
    "test": "npm run lint && npm run testonly"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "10.1.0",
    "babel-plugin-react-require": "3.1.3",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "chai": "3.5.0",
    "eslint": "4.19.1",
    "eslint-plugin-babel": "2.2.0",
    "eslint-plugin-react": "3.16.1",
    "mocha": "2.5.3",
    "mock-require": "3.0.3",
    "nodemon": "1.19.4",
    "react": "15.6.2",
    "react-dom": "15.6.2",
    "sinon": "1.17.7"
  },
  "peerDependencies": {
    "react": "0.14.x || 15.x.x || 16.x.x",
    "react-dom": "0.14.x || 15.x.x || 16.x.x"
  },
  "dependencies": {
    "babel-runtime": "6.26.0",
    "domready": "1.0.8"
  }
}
